{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*.{png,jpg,jpeg,gif,bmp,css,js}",
      "/api/*"
    ]
  },
  "mimeTypes": {
    ".json": "text/json"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
